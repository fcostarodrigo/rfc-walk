{
  "name": "rfc-walk",
  "version": "3.0.1",
  "description": "Transverse files recursively",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "precommit": "lint-staged && npm test",
    "test": "jest unit int --coverage",
    "pretest": "eslint src",
    "unit-test": "jest unit --watch",
    "format": "prettier --write \"**/*.js\""
  },
  "lint-staged": {
    "*.js": ["npm run format", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fcostarodrigo/rfc-walk.git"
  },
  "keywords": ["transverse", "files", "recursively", "list", "fs"],
  "author": "Rodrigo Fernandes da Costa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fcostarodrigo/rfc-walk/issues"
  },
  "homepage": "https://github.com/fcostarodrigo/rfc-walk#readme",
  "devDependencies": {
    "eslint": "^4.14.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^21.5.0",
    "eslint-plugin-prettier": "^2.4.0",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "prettier": "^1.9.2"
  }
}
